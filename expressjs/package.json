{
  "name": "safe-places-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "start": "node ./bin/www",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*.*",
    "test:int": "mocha test/integration/*.*",
    "test:unit": "mocha test/unit/*.*",
    "test:all": "mocha test/**/*.*",
    "format": "prettier --write **/*.{js,json}"
  },
  "pre-commit": [
    "format",
    "lint",
    "test"
  ],
  "dependencies": {
    "adm-zip": "^0.4.14",
    "atob": "^2.1.2",
    "bcrypt": "^4.0.1",
    "bluebird": "^3.7.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.4.0",
    "knex": "^0.20.15",
    "knex-postgis": "^0.12.0",
    "morgan": "~1.9.1",
    "ngeohash": "^0.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "wkx": "^0.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.0.0",
    "jshint": "^2.11.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "random-coordinates": "^1.0.1",
    "uuidv4": "^6.0.9"
  }
}
